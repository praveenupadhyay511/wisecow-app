{
  "family": "wisecow-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::311141542338:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "wise-cluster",
      "image": "311141542338.dkr.ecr.eu-west-1.amazonaws.com/wisecow-app",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4499,
          "hostPort": 4499,
          "protocol": "tcp",
       }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
